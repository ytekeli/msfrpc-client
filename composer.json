    "require-dev": {
        "phpstan/phpstan": "^0.12.3",
        "phpunit/phpunit": "^8.5.1 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ytekeli\\MsfRpcClient\\": "src/"
